{
  "name": "track-web-vitals-2",
  "version": "1.0.0",
  "description": "Track Web Vitals & visualize scores for performance insights.",
  "packageManager": "pnpm@9.1.0+sha256.22e36fba7f4880ecf749a5ca128b8435da085ecd49575e7fb9e64d6bf4fad394",
  "type": "module",
  "scripts": {
    "dev:engine": "dotenv -e .env.dev -- tsx --watch apps/engine/index.ts",
    "format": "prettier -w \"**/*.{ts,tsx,json,md}\" --ignore-unknown --cache",
    "migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "db:seed": "dotenv -e .env.prod -- tsx ./prisma/seed.ts",
    "db:reset": "dotenv -e .env.prod -- prisma db push --force-reset",
    "pull": "dotenv -e .env.prod -- prisma db pull",
    "generate-types": "dotenv -e .env.prod -- bash -c 'pnpm dlx supabase gen types typescript --db-url  $DATABASE_URL > ./prisma/dbTypes.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/type-provider-typebox": "^4.0.0",
    "@prisma/client": "5.13.0",
    "@sinclair/typebox": "^0.32.30",
    "@supabase/supabase-js": "^2.43.1",
    "close-with-grace": "^1.3.0",
    "dotenv-cli": "^7.4.2",
    "fastify": "^4.27.0",
    "kysely": "^0.27.3",
    "pg": "^8.11.5",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/pg": "^8.11.6",
    "kysely-supabase": "^0.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "supabase": "^1.165.0",
    "tsx": "^4.10.0",
    "typescript": "^5.4.5"
  }
}
